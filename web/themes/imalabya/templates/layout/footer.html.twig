{#
/**
* @file
* Default theme implementation to display the footer block.
*
* @see template_preprocess_block()
*
* @ingroup themeable
*/
#}

{% set year = "now"|date("Y") %}
<p>
	{% include "@imalabya/layout/partials/code.html.twig" %}
	{{ " with "|t }}
	{% include "@imalabya/layout/partials/love.html.twig" %}
	{% trans %}
	by Malabya &copy;
	{{ year }}. Powered by
	{% endtrans %}
	<a href="https://drupal.org">Drupal 9</a>
</p>
<div id="social">
	<h2 class="visually-hidden show-sm">Find me on the internet</h2>
	<a href="#">{% include "@imalabya/layout/partials/twitter.html.twig" %}
		<span class="visually-hidden">{{ "Twitter"|t }}</span>
	</a>
	<a href="#">{% include "@imalabya/layout/partials/drupal.html.twig" %}
		<span class="visually-hidden">{{ "Drupal"|t }}</span>
	</a>
	<a href="#">{% include "@imalabya/layout/partials/github.html.twig" %}
		<span class="visually-hidden">{{ "Github"|t }}</span>
	</a>
	<a href="#">{% include "@imalabya/layout/partials/linkedin.html.twig" %}
		<span class="visually-hidden">{{ "LinkedIn"|t }}</span>
	</a>
</div>
